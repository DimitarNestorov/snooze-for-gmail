{
	"name": "appengine",
	"version": "0.0.0",
	"main": "./dist/index.js",
	"license": "MIT",
	"scripts": {
		"rm-dist": "rimraf dist",
		"deploy": "gcloud app deploy --quiet app.yaml cron.yaml",
