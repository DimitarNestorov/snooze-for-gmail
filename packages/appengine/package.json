		"watch": "yarn run rm-dist && webpack-cli --mode production --watch",
		"start": "node ./dist/index.js"
	},
	"dependencies": {
		"await-to-js": "2.1.1",
		"express": "4.16.4",
		"firebase-admin": "6.4.0",
		"googleapis": "36.0.0",
		"luxon": "1.9.0"
	}
}
